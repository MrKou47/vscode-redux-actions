{
  "name": "vscode-redux-actions",
  "displayName": "redux-actions",
  "description": "autocomplete for redux-actions",
  "version": "0.0.1",
  "publisher": "mrkou47",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": ["Languages"],
  "keywords": [
    "react",
    "generator"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "cp -f -r ./src/templates ./out && tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "chai": "^4.1.2",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  }
}